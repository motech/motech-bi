    <connection>MotechOltp </connection>
    <sql>SELECT u.system_id as staff_id, nom.name,  pat.value as type, CAST(mid.identifier AS SIGNED) as motech_id, u.date_created as start_date 
                                                FROM openmrs.users u 
  JOIN (select person_id, concat(given_name,&apos; &apos;,family_name) as name 
          from openmrs.person_name
         where voided = 0
         group by person_id) nom on nom.person_id = u.user_id 
  LEFT JOIN (select pi.identifier, concat(n.given_name,&apos; &apos;,n.family_name) as name
              from openmrs.patient_identifier pi, openmrs.person_name n
              where pi.identifier_type = 3 
                and pi.patient_id = n.person_id and n.voided=0
                                and n.preferred = 0
              ) mid on mid.name = nom.name
  LEFT JOIN (select person_id, IF(value=&apos;&apos;,&apos;Field Agent&apos;,value) as value
          from openmrs.person_attribute  
         where person_attribute_type_id = 19) pat on pat.person_id = u.user_id
 where u.voided=0;</sql>
